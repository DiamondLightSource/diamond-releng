<?xml version="1.0" encoding="UTF-8"?>
<rm:rmap xmlns:bc="http://www.eclipse.org/buckminster/Common-1.0" xmlns:rm="http://www.eclipse.org/buckminster/RMap-1.0">
  <!-- In Buckminster (3.7 anyhow), a property in a CQuery overrides a property on the command line, which overrides a property in an RMap.
       In order to allow a value to be specified in a CQuery, but overridden on the command line, two different property names must be used.
       The "real" property (the one used throughout the RMap) can simply be overridden on the command line.
       A "shadow" property appears in the CQuery. This only overrides the real property if it is changed from its default value.
  -->
  <!--                              -->
  <!-- General properties           -->
  <!--                              -->
  <rm:propertyElement key="repository.git.checkout.root">
    <bc:constant value="${workspace.root}_git"/>  <!-- WARNING: never change this, many scripts assume the git checkout root is formed by appending "_git" to the workspace location -->
  </rm:propertyElement>
  <!-- ***** -->
  <rm:propertyElement key="buckminster.download.source">
    <bc:constant value="true"/>
  </rm:propertyElement>
  <!-- download.location.common can be "diamond" (DLS internal servers) [default] or "public" (internet-visible servers)
       The shadow property name in the CQuery is "download.location". -->
  <rm:propertyElement key="download.location">  <!-- defined here in case it is not defined in the CQuery -->
    <bc:constant value="diamond"/>
  </rm:propertyElement>
  <rm:propertyElement key="download.location.from.cquery.translated">
    <bc:replace>
      <bc:constant value="${download.location}"/>
      <bc:match pattern="^ *diamond *$" replacement="diamond"/>
      <bc:match pattern="^ *public *$" replacement="public"/>
      <bc:match pattern="^Default is 'diamond'.*$" replacement="diamond"/>
      <bc:match pattern="^Default is 'public'.*$" replacement="public"/>
      <bc:match pattern="^(?! *(diamond|public) *$).*$" replacement="unrecognised.download.location"/>
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="download.location.common">
    <bc:constant value="${download.location.from.cquery.translated}"/>
  </rm:propertyElement>
  <!-- ***** -->
  <rm:propertyElement key="training.repository.user">
    <bc:constant value="template"/>
  </rm:propertyElement>
  <!--                                                                -->
  <!-- Properties for Git repositories managed by gitolite at Diamond -->
  <!--                                                                -->
  <rm:propertyElement key="diamond.dascgit.host.port">  <!-- if using port forwarding, you will need to set this to "localhost:<port>" -->
    <bc:constant value="dasc-git.diamond.ac.uk"/>
  </rm:propertyElement>
  <rm:propertyElement key="diamond.dascgit.url">
    <bc:constant value="ssh://dascgitolite@${diamond.dascgit.host.port}"/>
  </rm:propertyElement>
  <!-- -->
  <!--                                                              -->
  <!-- Properties for Git repositories managed by Gerrit at Diamond -->
  <!--                                                              -->
  <!-- dascgerrit.authentication.common normally defaults "ssh", but can be overridden (for example, for Jenkins test jobs).
       The shadow property name in the CQuery is "dascgerrit.authentication". -->
  <!-- dascgerrit.username.common normally defaults to the current username, but can be overridden (for example, for Jenkins test jobs).
       This is not a security issue, since you need the matching private ssh key in order to gain access as that user
       The shadow property name in the CQuery is "dascgerrit.username". -->
  <rm:propertyElement key="dascgerrit.authentication">  <!-- defined here in case it is not defined in the CQuery -->
    <bc:constant value="ssh"/>
  </rm:propertyElement>
  <rm:propertyElement key="dascgerrit.authentication.from.cquery.translated">
    <bc:replace>
      <bc:constant value="${dascgerrit.authentication}"/>
      <bc:match pattern="^[Dd]efault.*$" replacement="ssh"/>
      <bc:match pattern="^ *ssh *$" replacement="ssh"/>
      <bc:match pattern="^ *anonymous *$" replacement="anonymous"/>
      <bc:match pattern="^(?! *(ssh|anonymous) *$).*$" replacement="unrecognised.dascgerrit.authentication"/>
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="dascgerrit.authentication.common">
    <bc:constant value="${dascgerrit.authentication.from.cquery.translated}"/>
  </rm:propertyElement>
  <!-- -->
  <rm:propertyElement key="dascgerrit.username">  <!-- defined here in case it is not defined in the CQuery -->
    <bc:constant value="default"/>
  </rm:propertyElement>
  <rm:propertyElement key="dascgerrit.username.from.cquery.translated">
    <bc:replace>
      <bc:constant value="${dascgerrit.username}"/>
      <bc:match pattern="^[Dd]efault.*$" replacement="remove-username"/>
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="dascgerrit.username.common">
    <bc:constant value="${dascgerrit.username.from.cquery.translated}"/>
  </rm:propertyElement>
  <rm:propertyElement key="dascgerrit.username.final">  <!-- if authentication is anonymous, ignore any username that was specified-->
    <bc:replace>
      <bc:constant value="${dascgerrit.authentication.common}--${dascgerrit.username.common}"/>
      <bc:match pattern="^anonymous--.*$" replacement="remove-username"/>
      <bc:match pattern="^ssh--" replacement="remove-prefix"/>  <!-- we do it this way because Buckminster does not allow an empty replacement -->
    </bc:replace>
  </rm:propertyElement>
  <!-- -->
  <rm:propertyElement key="dascgerrit.scheme">
    <bc:replace>
      <bc:constant value="${dascgerrit.authentication.common}"/>
      <bc:match pattern="anonymous" replacement="http"/>
      <bc:match pattern="^(?! *(ssh|http) *$).*$" replacement="unrecognised.dascgerrit.authentication.common"/>
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="diamond.dascgerrit.http.host.port">  <!-- if using port forwarding, you will need to set this to "localhost:<port>" -->
    <bc:constant value="gerrit.diamond.ac.uk:8081"/>
  </rm:propertyElement>
  <rm:propertyElement key="diamond.dascgerrit.ssh.host.port">  <!-- if using port forwarding, you will need to set this to "localhost:<port>" -->
    <bc:constant value="gerrit.diamond.ac.uk:29418"/>
  </rm:propertyElement>
  <!-- -->
  <rm:propertyElement key="diamond.dascgerrit.url.pass1">
    <bc:replace>
      <bc:constant value="${dascgerrit.scheme}://${dascgerrit.username.final}@${diamond.dascgerrit.${dascgerrit.scheme}.host.port}"/>
      <bc:match pattern="//remove-prefix" replacement="//"/>  <!-- we do it this way because Buckminster does not allow an empty replacement -->
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="diamond.dascgerrit.url">
    <bc:replace>
      <bc:constant value="${diamond.dascgerrit.url.pass1}"/>
      <bc:match pattern="//remove-username@" replacement="//"/>  <!-- we do it this way because Buckminster does not allow an empty replacement -->
    </bc:replace>
  </rm:propertyElement>
