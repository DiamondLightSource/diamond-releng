<?xml version="1.0" encoding="UTF-8"?>
<rm:rmap xmlns:bc="http://www.eclipse.org/buckminster/Common-1.0" xmlns:rm="http://www.eclipse.org/buckminster/RMap-1.0">
  <!--                              -->
  <!-- General properties           -->
  <!--                              -->
  <rm:propertyElement key="buckminster.download.source">
    <bc:constant value="true"/>
  </rm:propertyElement>
  <!-- In Buckminster (3.7 anyhow), a property in a CQuery overrides a property on the command line, which overrides a property in an RMap.
       In order to allow a value to be specified in a CQuery, but overridden on the command line, two different property names must be used.
       The "real" property (the one used throughout the RMap) can simply be overridden on the command line.
       A "shadow" property appears in the CQuery. This only overrides the real property if it is changed from its default value.
  -->
  <!-- ***** -->
  <!-- download.location.common can be "diamond" (DLS internal servers) [default] or "public" (internet-visible servers)
       The shadow property name in the CQuery is "download.location". -->
  <rm:propertyElement key="download.location">  <!-- defined here in case it is not defined in the CQuery -->
    <bc:constant value="diamond"/>
  </rm:propertyElement>
  <rm:propertyElement key="download.location.from.cquery.translated">
    <bc:replace>
      <bc:constant value="${download.location}"/>
      <bc:match pattern="^ *diamond *$" replacement="diamond"/>
      <bc:match pattern="^ *public *$" replacement="public"/>
      <bc:match pattern="^Default is 'diamond'.*$" replacement="diamond"/>
      <bc:match pattern="^Default is 'public'.*$" replacement="public"/>
      <bc:match pattern="^(?! *(diamond|public) *$).*$" replacement="unrecognised.download.location"/>
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="download.location.common">
    <bc:constant value="${download.location.from.cquery.translated}"/>
  </rm:propertyElement>
  <!-- ***** -->
  <rm:propertyElement key="libs.download.location">
    <bc:constant value="${download.location.common}"/>
  </rm:propertyElement>
  <rm:propertyElement key="springframework.download.location">
    <bc:constant value="${download.location.common}"/>
  </rm:propertyElement>
  <!-- ***** -->
  <rm:propertyElement key="diamond.dascgit.host.port">  <!-- if using port forwarding, you will need to set this to "localhost:<port>" -->
    <bc:constant value="dasc-git.diamond.ac.uk"/>
  </rm:propertyElement>
  <rm:propertyElement key="diamond.dascgit.url">
    <bc:constant value="ssh://dascgitolite@${diamond.dascgit.host.port}"/>
  </rm:propertyElement>
  <!-- -->
  <rm:propertyElement key="diamond.p2.host.port">  <!-- if using port forwarding, you will need to set this to "localhost:<port>" -->
    <bc:constant value="dawn.diamond.ac.uk"/>
  </rm:propertyElement>
  <rm:propertyElement key="diamond.p2.url">
    <bc:constant value="http://${diamond.p2.host.port}"/>
  </rm:propertyElement>
  <!-- ***** -->
  <!-- github.authentication.common can be "ssh" or "anonymous"
       The shadow property name in the CQuery is "github.authentication". -->
  <rm:propertyElement key="github.authentication">  <!-- defined here in case it is not defined in the CQuery -->
    <bc:constant value="ssh"/>
  </rm:propertyElement>
  <rm:propertyElement key="github.authentication.from.cquery.translated">
    <bc:replace>
      <bc:constant value="${github.authentication}"/>
      <bc:match pattern="^ *ssh *$" replacement="ssh"/>
      <bc:match pattern="^ *anonymous *$" replacement="anonymous"/>
      <bc:match pattern="^Default is 'ssh'.*$" replacement="ssh"/>
      <bc:match pattern="^Default is 'anonymous'.*$" replacement="anonymous"/>
      <bc:match pattern="^(?! *(ssh|anonymous) *$).*$" replacement="unrecognised.github.authentication"/>
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="github.authentication.common">
    <bc:constant value="${github.authentication.from.cquery.translated}"/>
  </rm:propertyElement>
  <rm:propertyElement key="github.anonymous.protocol">  <!-- can be "git" (fastest) or "http" (suitable if TCP port 9418 is blocked) -->
    <bc:constant value="git"/>
  </rm:propertyElement>
  <rm:propertyElement key="github.anonymous.url">
    <bc:constant value="${github.anonymous.protocol}://github.com/"/>
  </rm:propertyElement>
  <rm:propertyElement key="github.ssh.url">
    <bc:constant value="git@github.com:"/>
  </rm:propertyElement>
  <rm:propertyElement key="github.url">
    <bc:constant value="${github.${github.authentication.common}.url}"/>
  </rm:propertyElement>
  <!-- ***** -->
  <rm:propertyElement key="repository.git.checkout.root">
    <bc:constant value="${workspace.root}_git"/>  <!-- WARNING: do not casually change this, many scripts assume the git checkout root is formed by appending "_git" to the workspace location -->
  </rm:propertyElement>
  <rm:propertyElement key="training.repository.user">
    <bc:constant value="template"/>
  </rm:propertyElement>
