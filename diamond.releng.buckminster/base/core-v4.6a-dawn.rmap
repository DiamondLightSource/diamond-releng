        Components in Git repositories (note: patterns should match those in the CQuery advisor nodes)
        Please keep locators in alphabetical order of repository name
  -->
  <rm:locator pattern="^org\.dawb\.workbench\.build$" searchPathRef="dawn_build"/>
  <rm:locator pattern="^org\.dawb\.(common\.(python|services|ui|util)|(gda|pydev)\.extensions|hdf5(\.editor)?|workbench\.examples)$" searchPathRef="dawn_common"/>
