      <bc:match pattern="^Default is 'anonymous'.*$" replacement="anonymous"/>
      <bc:match pattern="^(?! *(ssh|anonymous) *$).*$" replacement="unrecognised.github.authentication"/>
    </bc:replace>
  </rm:propertyElement>
  <rm:propertyElement key="github.authentication.common">
    <bc:constant value="${github.authentication.from.cquery.translated}"/>
  </rm:propertyElement>
  <rm:propertyElement key="github.anonymous.protocol">  <!-- can be "git" (fastest) or "https" (suitable if TCP port 9418 is blocked) -->
